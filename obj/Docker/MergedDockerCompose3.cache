name: dockercompose6770212065720952684
services:
  cachedb:
    container_name: cachedb
    image: redis:alpine
    networks:
      default: null
    ports:
    - mode: ingress
      target: 6379
      published: "6379"
      protocol: tcp
    restart: always
  education.api:
    build:
      context: C:\Education\Education
      dockerfile: Education.API\Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Education.API
      target: base
    container_name: education.api
    depends_on:
      cachedb:
        condition: service_started
      educationdb:
        condition: service_started
    entrypoint:
    - C:\remote_debugger\x64\msvsmon.exe
    - /noauth
    - /anyuser
    - /silent
    - /nostatus
    - /noclrwarn
    - /nosecuritywarn
    - /nofirewallwarn
    - /nowowwarn
    - /timeout:2147483646
    - /LogDebuggeeOutputToStdOut
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      ApiSettings:GatewayAddress: http://ocelotapigw
      CacheSettings:ConnectionString: cachedb:6379
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      DatabaseSettings:ConnectionString: Server=educationdb;Port=5432;Database=Education;User
        Id=admin;Password=admin1234;
      NUGET_PACKAGES: C:\.nuget\packages
    image: educationapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.killprogram: C:\remote_debugger\x64\utils\KillProcess.exe
        dotnet.exe
      com.microsoft.visualstudio.debuggee.program: '"C:\Program Files\dotnet\dotnet.exe"'
      com.microsoft.visualstudio.debuggee.workingdirectory: C:\app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "5010"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\murat.cakmak2\AppData\Roaming/ASP.NET/Https
      target: C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\murat.cakmak2\AppData\Roaming/Microsoft/UserSecrets
      target: C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Education\Education\Education.API
      target: C:\app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\murat.cakmak2\onecoremsvsmon\17.1.1414.0
      target: C:\remote_debugger
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Education\Education
      target: C:\src
      bind:
        create_host_path: true
  educationdb:
    container_name: educationdb
    environment:
      POSTGRES_DB: Education
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    networks:
      default: null
    ports:
    - mode: ingress
      target: 5432
      published: "5432"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: postgres_data
      target: /var/lib/postgresql/data
      volume: {}
  ocelotapigw:
    build:
      context: C:\Education\Education
      dockerfile: Education.ApiGateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Education.ApiGateway
      target: base
    container_name: ocelotapigw
    depends_on:
      education.api:
        condition: service_started
    entrypoint:
    - C:\remote_debugger\x64\msvsmon.exe
    - /noauth
    - /anyuser
    - /silent
    - /nostatus
    - /noclrwarn
    - /nosecuritywarn
    - /nofirewallwarn
    - /nowowwarn
    - /timeout:2147483646
    - /LogDebuggeeOutputToStdOut
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: "1"
      NUGET_PACKAGES: C:\.nuget\packages
    image: ocelotapigw:dev
    labels:
      com.microsoft.visualstudio.debuggee.killprogram: C:\remote_debugger\x64\utils\KillProcess.exe
        dotnet.exe
      com.microsoft.visualstudio.debuggee.program: '"C:\Program Files\dotnet\dotnet.exe"'
      com.microsoft.visualstudio.debuggee.workingdirectory: C:\app
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "8010"
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Education\Education\Education.ApiGateway
      target: C:\app
      bind:
        create_host_path: true
    - type: bind
      source: C:\Education\Education
      target: C:\src
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\murat.cakmak2\onecoremsvsmon\17.1.1414.0
      target: C:\remote_debugger
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\murat.cakmak2\AppData\Roaming/ASP.NET/Https
      target: C:\Users\ContainerUser\AppData\Roaming\ASP.NET\Https
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\murat.cakmak2\AppData\Roaming/Microsoft/UserSecrets
      target: C:\Users\ContainerUser\AppData\Roaming\Microsoft\UserSecrets
      read_only: true
      bind:
        create_host_path: true
networks:
  default:
    name: dockercompose6770212065720952684_default
volumes:
  postgres_data:
    name: dockercompose6770212065720952684_postgres_data